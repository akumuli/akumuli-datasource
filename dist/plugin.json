{
  "type": "datasource",
  "name": "Akumuli",
  "id": "akumuli-datasource",

  "metrics": true,
  "defaultMatchFormat": "pipe",
  "annotations": true,
  "alerting": true,

  "info": {
    "version": "1.0.3",
    "description": "Datasource plugin for Akumuli time-series database",
    "author": {
      "name": "Akumuli Authors",
      "url": "https://akumuli.org"
    },
    "logos": {
      "small": "img/logo.svg.png",
      "large": "img/logo.svg.png"
    },
    "keywords": [
      "akumuli",
      "tsdb",
      "time series",
      "timeseries",
      "time-series"
    ]
  },
  "dependencies": {
    "grafanaVersion": "4.x.x",
    "plugins": []
  }
}
